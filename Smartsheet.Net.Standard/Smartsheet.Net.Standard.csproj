<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Smartsheet.Net.Standard</PackageId>
    <PackageVersion>2.0.9</PackageVersion>
    <Authors>Austen DeWolf, Bobby Dick, Seth Martin</Authors>
    <Description>Smartsheet C# SDK to facilitate HTTP Requests operations against the Smartsheet API (api.smartsheet.com)</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Introduces the loadAll parameter for GET Workspace calls and adds convenience helpers for updating a single Cell or Row.</PackageReleaseNotes>
    <Copyright>Copyright 2018 (c)</Copyright>
    <PackageTags>smartsheet professional-services sdk</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Interfaces\IPrimitiveObjectValue.cs" />
  </ItemGroup>
</Project>
